class HelloWorld {
    public static void main(String[] args) {
       int  a[]={50,60,70,80,200,-1,600,20};
       int min=a[0],max=a[0];
       for(int i=1;i<a.length;i++){
           if(a[i]>max){
               max=a[i];
           }
           if(a[i]<min)
           {
               min=a[i];
           }
       }
        System.out.println("Maximum number of array :"+max);
        System.out.println("Minimum number of array : "+min);
    }
}
